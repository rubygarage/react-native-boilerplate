{
  "name": "DrunkJesusMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --format codeframe ./ && yarn lint:css",
    "lint:css": "stylelint './src/**/*.js'",
    "test:circle": "jest --coverage --maxWorkers=3"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.10.3",
    "@react-native-community/netinfo": "^5.9.1",
    "@react-native-community/picker": "^1.6.1",
    "@sentry/react-native": "^1.5.0",
    "axios": "^0.19.1",
    "formik": "^2.1.4",
    "json-api-normalizer": "^0.4.16",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "ramda": "^0.27.0",
    "react": "16.9.0",
    "react-intl": "^3.12.0",
    "react-intl-formatted-duration": "^3.1.0",
    "react-intl-redux": "^2.2.0",
    "react-native": "0.61.5",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-incall-manager": "^3.2.7",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-permissions": "^2.1.5",
    "react-native-safe-area-context": "^2.0.0",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^2.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.3.9",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logic": "^3.0.0",
    "redux-object": "^0.5.10",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "mockdate": "^3.0.2",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.5.0",
    "stylelint-config-react-native-styled-components": "^0.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.2.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>setup-tests.js"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native)/.+"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!lib/vendor/**",
      "!babel.config.js",
      "!react-native.config.js",
      "!metro.config.js",
      "!index.js",
      "!**/state/configureStore.js",
      "!**/src/stubs/**",
      "!**/src/constants/permissions/**",
      "!**/lib/janus/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/.*/__mocks__"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "globalSetup": "<rootDir>/globalTestSetup.js"
  }
}
